<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tabs"
    style="@style/Theme.MyApplication"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:layout_editor_absoluteX="-25dp"
    tools:layout_editor_absoluteY="11dp"
    tools:visibility="visible">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="339dp"
        android:layout_height="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.959"
        app:srcCompat="@drawable/tabs_container" />

    <ToggleButton
        android:id="@+id/tabs_folder"
        android:layout_width="45dp"
        android:layout_height="19dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/tabs_toggle"
        android:textOff=" "
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />

    <ToggleButton
        android:id="@+id/Posting"
        android:layout_width="45dp"
        android:layout_height="19dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/google"
        android:textOff=" "
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toEndOf="@+id/tabs_folder"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />

    <ToggleButton
        android:id="@+id/Client"
        android:layout_width="45dp"
        android:layout_height="19dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/google"
        android:textOff=" "
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toEndOf="@+id/Posting"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />

    <ToggleButton
        android:id="@+id/Schedule"
        android:layout_width="45dp"
        android:layout_height="19dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/google"
        android:textOff=" "
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toEndOf="@+id/Client"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />

    <ToggleButton
        android:id="@+id/Settings"
        android:layout_width="45dp"
        android:layout_height="19dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/google"
        android:textOff=" "
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintStart_toEndOf="@+id/Schedule"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>